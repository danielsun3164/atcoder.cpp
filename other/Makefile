OBJS=_atc001 _atc002 _joi2006yo _joi2006ho _joi2007yo _joi2007ho _joi2008yo _joisc2007 _loadchecking _typical90 _zone2021

all:	$(OBJS)

_atc001:
	make -C atc001

_atc002:
	make -C atc002

_joi2006yo:
	make -C joi2006yo

_joi2006ho:
	make -C joi2006ho

_joi2007yo:
	make -C joi2007yo

_joi2007ho:
	make -C joi2007ho

_joi2008yo:
	make -C joi2008yo

_joisc2007:
	make -C joisc2007

_loadchecking:
	make -C loadchecking

_typical90:
	make -C typical90

_zone2021:
	make -C zone2021

clean:
	make -C atc001 clean
	make -C atc002 clean
	make -C joi2006yo clean
	make -C joi2006ho clean
	make -C joi2007yo clean
	make -C joi2007ho clean
	make -C joi2008yo clean
	make -C joisc2007 clean
	make -C loadchecking clean
	make -C typical90 clean
	make -C zone2021 clean
